int main()
{
    int base;
    printf("enter the number :");
    scanf("%d",&base);

    int power;
    printf("enter the number :");
    scanf("%d",&power);
    int result =1;
    int i=1;
    while(i<=power)
    {
        result=result*base;
        i++;

    }
    printf("%d",result);
    return 0;
}


#include<stdio.h>
int main()
{
    int sp;
    printf("enter the value of sp :");
    scanf("%d",&sp);
    int ep;
    printf("enter the value of sp :");
    scanf("%d",&ep);

    int fact=1;
    while(sp>=ep)
    {
        fact=fact*sp;
        sp=sp-1;
    }
        printf("%d",fact);
    
    return 0;
}
